source 'http://rubygems.org'

# Bundle edge Rails instead:
gem 'rails', :git => 'git://github.com/rails/rails.git'
gem 'pg'
gem 'coffee-script'

# Deploy with Capistrano
gem 'capistrano'
gem 'capistrano-ext'

group :development, :test do
  gem 'ruby-debug19'
end

group :staging, :production do
  gem 'passenger'
end
